        <content src="Text/to_sladke_jmeno_beatrice_031.xhtml#_idParaDest-26"/>
      </navPoint>
      <navPoint id="navPoint-28" playOrder="28">
        <navLabel>
          <text>Smějete se mé tváři s ostatními…</text>
