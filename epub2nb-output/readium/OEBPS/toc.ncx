        <content src="Text/to_sladke_jmeno_beatrice_018.xhtml#_idParaDest-14"/>
      </navPoint>
      <navPoint id="navPoint-15" playOrder="15">
        <navLabel>
          <text>Viděl jsem tuhle hlouček krásných paní…</text>
        </navLabel>
        <content src="Text/to_sladke_jmeno_beatrice_019.xhtml#_idParaDest-15"/>
      </navPoint>
      <navPoint id="navPoint-16" playOrder="16">
        <navLabel>
          <text>Z očí mé paní sálá tolik jasu…</text>
