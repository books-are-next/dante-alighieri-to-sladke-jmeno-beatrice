        <content src="Text/to_sladke_jmeno_beatrice_029.xhtml#_idParaDest-24"/>
      </navPoint>
      <navPoint id="navPoint-26" playOrder="26">
        <navLabel>
          <text>Má balato, jdi Lásku vyhledat…</text>
