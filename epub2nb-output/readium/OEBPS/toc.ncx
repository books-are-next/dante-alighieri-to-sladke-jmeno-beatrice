        <content src="Text/to_sladke_jmeno_beatrice_025.xhtml#_idParaDest-20"/>
      </navPoint>
      <navPoint id="navPoint-22" playOrder="22">
        <navLabel>
          <text>Vy , co se cestou lásky ubíráte…</text>
