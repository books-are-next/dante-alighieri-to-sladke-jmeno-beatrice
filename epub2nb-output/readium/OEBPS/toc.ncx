        <content src="Text/to_sladke_jmeno_beatrice_053.xhtml#_idParaDest-47"/>
      </navPoint>
      <navPoint id="navPoint-49" playOrder="49">
        <navLabel>
          <text>Běda, z těch dum, jež srdce v sobě skrývá…</text>
