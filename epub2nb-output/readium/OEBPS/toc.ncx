        <content src="Text/to_sladke_jmeno_beatrice_056.xhtml#_idParaDest-49"/>
      </navPoint>
      <navPoint id="navPoint-51" playOrder="51">
        <navLabel>
          <text>Až za nejvyšší sféru všehomíra…</text>
        </navLabel>
        <content src="Text/to_sladke_jmeno_beatrice_057.xhtml#_idParaDest-50"/>
