        <content src="Text/to_sladke_jmeno_beatrice_074.xhtml#_idParaDest-66"/>
      </navPoint>
      <navPoint id="navPoint-68" playOrder="68">
        <navLabel>
          <text>Mám-li si tedy, Lásko, naříkat…</text>
