        <content src="Text/to_sladke_jmeno_beatrice_055.xhtml#_idParaDest-48"/>
      </navPoint>
      <navPoint id="navPoint-50" playOrder="50">
        <navLabel>
          <text>Poutníci, kteří jdete zamyšlení…</text>
